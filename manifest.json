{
    "manifest_version": 2,
    "name": "blocker",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": ["webRequest",
                    "webRequestBlocking", 
                    "<all_urls>"],
    "background": {
        "scripts": ["block.js"],
        "persistent": true
    },
    "browser_action": {
        "default_popup": "block.html"
    }
}
